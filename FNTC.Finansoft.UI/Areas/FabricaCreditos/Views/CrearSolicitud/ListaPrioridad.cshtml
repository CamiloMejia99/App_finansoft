

@model IEnumerable<FNTC.Finansoft.Accounting.DTO.FabricaCreditos.FCConfiguracion>


<select class="form-control" id="idPrioridad" name="idPrioridad">
    <option value="">Seleccionar</option>
    @foreach (var item in Model)
    {
        <option value="@item.idConfiguracion">|Prioridad: @item.nomConfig |TiempRespMin: @item.tiempoRespuestaSolMin (H) |TiempResMax: @item.tiempoRespuestaSolMax (H) |TiempMaxAprobacion: @item.tiempoMaxOtorgarCredito (D) |EdadMin: @item.edadMinCredito |EdadMax: @item.edadMaxCredito |</option>
    }
</select>