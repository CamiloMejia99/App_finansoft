

@model IEnumerable<FNTC.Finansoft.Accounting.DTO.DescuentosNomina.TipoPagos>

@{
    var IdOrden = ViewBag.IdOrden;
}

<select class="form-control" id="Orden" name="Orden" value="@IdOrden">
    <option value="@IdOrden">|Orden: @IdOrden</option>
    @foreach (var item in Model)
    {
        <option value="@item.Orden">|@item.NombrePago |Orden: @item.Orden</option>
    }
</select>
