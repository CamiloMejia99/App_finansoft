@model IEnumerable<FNTC.Finansoft.Accounting.DTO.Nomina.SeleccionCuenta>



<div class="modal-body">
    

    <table class="table table-striped table-bordered dataTable no-footer" style="background-color: #FFF;">

        <tr>
            <th class="columnatablaazul2">
                <label for="CodigoCuenta">PLANO</label>
            </th>
            <th class="columnatablaazul2">
                <label for="NombreCuenta">EMPRESA</label>
            </th>
            <th class="columnatablaazul2">
                <label for="NombreCuenta">FECHA</label>
            </th>

            <th>
                <label for="Editar">VISUALIZAR</label>
            </th>

        </tr>

        @foreach (var item in Model)
        {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CODIGO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cuenta.NOMBRE)
        </td>
        <td>
            
        </td>

        <td>
            <button class="editartablas" onclick=" $('#divmodal').load('@Url.Action("Edit","SeleccionCuentas",new { id=item.ID})')"><i class="fa fa-pencil" aria-hidden="true"></i></button>
        </td>


    </tr>
        }

    </table>

</div>
