@model FNTC.Finansoft.Accounting.DTO.Nomina.ClaseDePlano
@if (ViewBag.guardado == "S")
{
    <script>
        recargar();
    </script>
}
else
{

    <div class="modal-content">
        <div class="modal-header">

            <h4 align="center" class="modal-title"> <b>ELIMINAR PLANO</b></h4>
        </div>
        <div class="modal-body">
            <center>
                <div>
                    <i class="fa fa-ban iconosmenu" aria-hidden="true"></i> <h4>¿Esta Seguro de Eliminar el Plano?</h4>
                </div>
            </center>
            <div>


                <dl class="dl-horizontal"></dl>


                @using (Html.BeginForm("Delete", "ClaseDePlanos", new { area = "Nomina" }, FormMethod.Post, new { id = "theFormDelete", autocomplete = "off" }))

                {
                    <center>
                        @Html.AntiForgeryToken()
                        @Html.HiddenFor(model => model.ID)
                        <div class="form-actions no-color">
                            <input type="button" value="Eliminar" class="btn btn-danger" onclick="Delete();" />  |
                            <button type="button" id="close" class="btn btn-default" data-dismiss="modal"><i class="fa fa-times" aria-hidden="true"></i> Cerrar</button>

                        </div>
                    </center>
                }
            </div>
        </div>
    </div>


}
