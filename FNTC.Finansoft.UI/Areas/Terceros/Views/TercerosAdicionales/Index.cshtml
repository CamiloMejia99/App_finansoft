@model IEnumerable<FNTC.Finansoft.Accounting.DTO.Terceros.InfoTerceroAdicional>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/datatables.boostrap.min.css" rel="stylesheet" />
<script src="~/Scripts/datatable.js"></script>
<link href="~/Content/font-awesome.min.css" rel="stylesheet" />
<script src="~/Content/tools/direccion/Nomemclaturas.min.js"></script>

<div class="container" style="margin-top:2%; background: rgb(248, 248, 248);">

    <ul class="col-md-offset-2 nav nav-tabs" style="margin-top:0%;border: none; margin-left: 2%;">
        <li class="active"><a href="#" class="tabsnota"><i class="fa fa-file-text" aria-hidden="true"></i> Terceros Datos Adicionales</a></li>
        <li><a href="/Terceros/InfoTerceroFinancieras" class="tabsnota"><i class="fa fa-file-text" aria-hidden="true"></i>Terceros Datos Financieros </a></li>

    </ul>

    <div class="tab-content contenedortabs">
        <div id="menu1" class="tab-pane fade in active">
            <div class="col-md-12"><h3 style="border-bottom: 5px solid #2B7785 !important; width: 100%; color: #2B7785 !important; padding: 9px 20px 15px 0px !important;margin-bottom: 31px !important;">Datos Adicionales Terceros</h3></div>
            <div class="col-md-12">
                <div class="col m-5">
                    <br />
                    <a href="/Dashboard/Default" class="btn btn-primary" data-toggle="modal">
                        Regresar
                    </a>
                    <a href="/Terceros/TercerosAdicionales/create" class="btn btn-primary" data-toggle="modal">
                        Crear
                    </a>
                </div>
                <br />

                <div class="table-responsive col-md-12">
                    <div class="col-md-12">
                        <table id="TablaTercerosAdicional" class="table table-striped table-bordered dataTable no-footer" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th class="columnatablaazul2">Nit</th>
                                    <th class="columnatablaazul2">Nombre</th>
                                    <th class="columnatablaazul">Nivel de Estudio</th>
                                    <th class="columnatablaazul">Estrato</th>
                                    <th class="columnatablaazul2">Ocupación</th>
                                    <th class="columnatablaazul">Personas a cargo</th>
                                    <th class="columnatablaazul2">Tipo Contrato</th>
                                    <th class="columnatablaazul">Editar</th>
                                    <th class="columnatablaazul">eliminar</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>


            </div>

        </div>

    </div>
</div>

<script type="text/javascript" class="init">
    $(document).ready(function () {
        $('#TablaTercerosAdicional').dataTable({
            "ajaxSource": '../terceros/TercerosAdicionales/GetTercerosAdicionales',
            "language": {
                "url": "//cdn.datatables.net/plug-ins/1.10.12/i18n/Spanish.json"
            }
        });
    });
</script>




