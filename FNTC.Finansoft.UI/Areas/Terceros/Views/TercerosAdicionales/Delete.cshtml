@model FNTC.Finansoft.Accounting.DTO.Terceros.InfoTerceroAdicional

@{
    ViewBag.Title = "Eliminar Datos Terceros Adicionales";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container" style="margin-top:2%; background: rgb(248, 248, 248);">


<ul class="col-md-offset-2 nav nav-tabs" style="margin-top:0%;border: none; margin-left: 2%;">
    <li><a href="/Terceros/TercerosAdicionales" class="tabsnota"><i class="fa fa-file-text" aria-hidden="true"></i> Terceros Datos Adicionales</a></li>
    <li><a href="/Terceros/InfoTerceroFinancieras" class="tabsnota"><i class="fa fa-file-text" aria-hidden="true"></i>Terceros Datos Financieros </a></li>

</ul>

<div class="tab-content contenedortabs">
    <div id="menu1" class="tab-pane fade in active">
        <div class="col-md-5"><h3 style="border-bottom: 5px solid #2B7785 !important; width: 100%; color: #2B7785 !important; padding: 9px 20px 15px 0px !important;margin-bottom: 31px !important;">Eliminar Datos Financieros</h3></div>
        <div class="col-md-12">
            <div class="col m-5">
                <br />
                <a href="/Terceros/TercerosAdicionales" class="btn btn-primary" >
                    Regresar
                </a>
            </div>

            <h3>¿Estás seguro que quieres eliminar esto?</h3>
            <div>

                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.Contrato.TipoContrato)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Contrato.TipoContrato)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.estrato.Estrato)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.estrato.Estrato)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.NivelEstudio.Nestudio)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.NivelEstudio.Nestudio)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Tercero.NIT)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Tercero.NIT)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.PersonasCargo)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.PersonasCargo)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Ocupacion)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Ocupacion)
                    </dd>


                </dl>

                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="form-actions no-color">


                        <button type="submit" value="Delete" class="btn btn-secondary"><i class="btn btn-danger glyphicon glyphicon-trash" aria-hidden="true"></i> Eliminar </button>

                    </div>
                }
            </div>



        </div>

    </div>

</div>
</div>






