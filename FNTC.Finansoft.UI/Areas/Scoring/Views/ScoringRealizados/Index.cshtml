@model IEnumerable<FNTC.Finansoft.Accounting.DTO.Scoring.ScoringScoringRealizado>

@{
    Layout = "~/Views/Shared/_LayoutActivosFijos.cshtml";
}

@{
    ViewBag.Title = "Scoring Realizados";
}
<link href="~/Content/css/chosen.css" rel="stylesheet" />

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>

    <div class="row">

        <div class="col-lg-12">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    Scoring Realizados
                </div>
                <div class="panel-body">
                    <div class="row">

                        <table class="table">
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.Cedula)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Nombre)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Puntaje)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.PuntajeDatacredito)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.PuntajeTotal)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.CapacidadPago)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.PorcentajeEndeudamiento)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Fecha)
                                </th>
                                <th>
                                    Eliminar Registro
                                </th>
                            </tr>

                            @foreach (var item in Model)
                            {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Cedula)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Nombre)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Puntaje)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.PuntajeDatacredito)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.PuntajeTotal)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.CapacidadPago)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.PorcentajeEndeudamiento)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Fecha)
                                </td>
                                <td>
                                    <button class="eliminartablas" onclick=" $('#divmodal').load('@Url.Action("Delete","ScoringRealizados",new { id=item.id })')"><i class="fa fa-trash" aria-hidden="true"></i></button>
                                </td>

                            </tr>
                            }

                        </table>

                    </div>
                </div>
            </div>

        </div>
    </div>
    <div>
        @Html.ActionLink("Regresar a la Lista", "../Scoring/")
    </div>
</body>

</html>

<script>
        function Delete() {

        var formData = $('#FormularioSeleccion').serialize();


        $.ajax({
            type: "POST",
            url: "@Url.Action("Delete", "ScoringRealizados")",
            data: formData,
            success: function () {              
                $('#centro').modal('hide');
                location.reload();
            }
        });
    }
     

</script>

<div id="divmodal"></div>
