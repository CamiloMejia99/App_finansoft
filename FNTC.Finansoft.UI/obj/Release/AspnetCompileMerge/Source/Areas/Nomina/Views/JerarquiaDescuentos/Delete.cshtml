@model FNTC.Finansoft.Accounting.DTO.Nomina.JerarquiaDescuento



@if (ViewBag.guardado == "S")
{
    <script>
         recargar();
        
    </script>
}
else
{
    <div id="centro" class="modal fade" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    
                    <h4 class="modal-title">Jerarquía Descuentos de Productos de Nomina</h4>
                </div>
                <div class="modal-body">
                        <center>
                          <h3>Seguro desea eliminar esta cuenta?</h3>
                        </center>
                    <div>

                        <hr />
                        <dl class="dl-horizontal">
                      
                        </dl>
                        @using (Html.BeginForm("Delete", "JerarquiaDescuentos", new { area = "Nomina" }, FormMethod.Post, new { id = "theFormDelete", autocomplete = "off" }))

                        {

                                     @Html.AntiForgeryToken()
                             <center>
                                  @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                   @Html.HiddenFor(model => model.CODIGO)
                                <div class="form-actions no-color">
                                   <input type="button" value="Eliminar" class="btn btn-danger" onclick="Delete();" />  |
                                    <button type="button" id="close" class="btn btn-default" data-dismiss="modal"><i class="fa fa-times" aria-hidden="true"></i> Cerrar</button>
                                </div>
                            </center>
                                }
                                @if (ViewBag.Error != null && ViewBag.Error != "")
                                {
                                    <div class="text-danger">
                                        @ViewBag.Error
                                    </div>}
                    </div>
                </div>

            </div>

        </div>
    </div>

    <script>



        $(document).ready(function () {
            inicializarFormulario();
            $('#centro').modal('show');
        });
     
    </script>

}
