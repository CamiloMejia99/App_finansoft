    $("#Nit_CC").blur(function () {
        var texto = $(this).val();
        $.post("@Url.Action("CedulaValida")", {id :texto}, function(res){
            if (!res) {
                ban = false;                
                swal({
                    title: "Alerta",
                    type: "info",
                    text: "Ya existe la cedula digitada",
                    confirmButtonText: "Ok!",
                }, function () {
                    window.location.reload();
                });
                setTimeout(function () {
                    window.location.reload();
                }, 3000);
            } else {
                ban = true;
            }
        });
        
    });


