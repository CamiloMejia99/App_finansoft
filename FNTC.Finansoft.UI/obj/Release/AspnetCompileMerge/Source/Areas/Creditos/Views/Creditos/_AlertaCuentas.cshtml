@model  FNTC.Finansoft.Accounting.DTO.MCreditos.ViewModelDesembolso

<div class="modal-dialog modal-lg">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h4 class="modal-title">Desembolso</h4>
            @*<a class="mostrar" name="mostrar">.</a>*@
        </div>

        <div class="modal-body">
            <dl class="dl-horizontal"></dl>
            <input type="text" value="@Model.NOMBRE" id="alerta" hidden="hidden">           
        </div>
    </div>
</div>

<script>
    $(document).ready(function () {       
        var alerta = $("#alerta").val();
        swal({
            title: "Alerta",
            type: "error",
            text: alerta,
        })    
    });
</script>