@model FNTC.Finansoft.Accounting.DTO.OperativaDeCaja.Caja

@{
    ViewBag.Title = "Detalles de Caja";
    Layout = "~/Views/Shared/_Modal.cshtml";
}

<div class="modalheadergeneral" style="margin-top: -6%;"><h4><i class="fas fa-file-alt"></i>&nbsp;&nbsp;Detalles de Caja</h4></div>
<hr />
<h4 class="center">Datos de @Html.DisplayFor(model => model.Nombre_caja)</h4>
<div class="center contenido">
    <div class="dl-horizontal">
        <div class="row">
            <p class="col-md-6"><b>Código:</b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Html.DisplayFor(model => model.Codigo_caja)</p>
            <p class="col-md-6"><b>Nombre de Caja:</b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Html.DisplayFor(model => model.Nombre_caja)</p>
        </div>
        <div class="row">
            <p class="col-md-6"><b>Consecutivo Inicial:</b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Html.DisplayFor(model => model.Consecutivo_ini)</p>
            <p class="col-md-6"><b>Consecutivo Final:</b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Html.DisplayFor(model => model.consecutivo_fin)</p>
        </div>
        <div class="row">
            <p class="col-md-6"><b>Consecutivo Actual:</b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Html.DisplayFor(model => model.consecutivo_actual)</p>
            <p class="col-md-6"><b>Tope Máximo de Caja:</b>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Html.DisplayFor(model => model.TopeMaximo_caja)</p>
        </div>
    </div>
</div>
<div class="modal-footer">
	<a href="~/OperativaDeCaja/Cajas/Index" class="btn btn-inline btn-small btn-danger">Cerrar</a>
</div>
