@model IEnumerable<FNTC.Finansoft.Accounting.DTO.Nomina.JerarquiaDescuento>



<table class="table table-striped table-bordered dataTable no-footer" style="background-color: #FFF;">
    <tr>
        <th class="columnatablaazul2">
            @Html.DisplayNameFor(model => model.CODIGO)
        </th>
        <th class="columnatablaazul">
           <h5> Nombre cuenta</h5>
         </th>
        <th class="columnatablaazul2">
            @Html.DisplayNameFor(model => model.ORDEN)
        </th>

        <th></th>
        <th></th>
       
      
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CODIGO)
        </td>
        <td>

            @Html.DisplayFor(modelItem => item.Cuenta.NOMBRE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ORDEN)
        </td>


        <td>
            <button class="eliminartablas" onclick=" $('#divmodal').load('@Url.Action("Delete","JerarquiaDescuentos",new { id=item.CODIGO})')"><i class="fa fa-trash" aria-hidden="true"></i></button>
        </td>
         <td>
            <button class="editartablas" onclick=" $('#mostrar').load('@Url.Action("Move",new { id = item.CODIGO, action_ = "Subir" })')"><i class="fa fa-level-up" aria-hidden="true"></i></button>
            <button class="editartablas" onclick=" $('#mostrar').load('@Url.Action("Move",new { id = item.CODIGO, action_ = "Bajar" })')"><i class="fa fa-level-down" aria-hidden="true"></i></button>

          </td>




    </tr>
    }

</table>
