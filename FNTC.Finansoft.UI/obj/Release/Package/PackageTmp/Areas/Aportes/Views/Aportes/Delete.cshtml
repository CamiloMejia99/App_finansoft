@model FNTC.Finansoft.Accounting.DTO.FormularioVinculacion.formatoVinculacion

@{
    Layout = "~/Views/Shared/_Layoutahorros.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
<body>
    <h3>Seguro desea eliminar este formulario?</h3>
    <div>
        <table>
            <tr>
                <td>
                    @Html.DisplayFor(model => model.nombreaso)
                </td>
                <td>
                    :
                </td>
                <td>
                    @Html.DisplayFor(model => model.cedulaaso)
                </td>
            </tr>
        </table>
        <hr />

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Eliminar" class="btn btn-danger" /> |
                @Html.ActionLink("Regresar a la lista", "Index")
            </div>
        }
    </div>
</body>
</html>
