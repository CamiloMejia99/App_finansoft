

@model IEnumerable<FNTC.Finansoft.Accounting.DTO.Terceros.CargoEmpresaTer>

@{
    var cargo = ViewBag.cargo;
    var nombreCargo = ViewBag.CargoNombre;
 }

<select class="form-control chosen-select-single" id="CARGO" name="CARGO">
    <option value="@cargo">@nombreCargo</option>
    @foreach (var item in Model)
    {
        <option value="@item.IDCargo"> @item.NombreCargo</option>

    }

</select>