
@model IEnumerable<FNTC.Finansoft.Accounting.DTO.DescuentosNomina.ConformacionDeLosPlanos>
@{ 
    var orden = ViewBag.idOrden;
}
<select class="form-control" id="OrdenCampo" name="OrdenCampo">

    @foreach (var item in Model)
    {
        <option value="@item.OrdenCampo" @(item.OrdenCampo == orden ? "selected" : "")>|Orden: @item.OrdenCampo|</option>

    }
</select>