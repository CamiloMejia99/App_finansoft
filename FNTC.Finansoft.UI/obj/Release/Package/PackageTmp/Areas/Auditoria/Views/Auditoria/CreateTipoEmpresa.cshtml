@model FNTC.Finansoft.Accounting.DTO.Auditoria.TipoEmpresa

@{
    Layout = "~/Views/Shared/_Layoutahorros.cshtml";
}

<html>
<head>
    <meta name="viewport" content="width=device-width">
    <script src="~/Scripts/jquery2-1-4.min.js"></script>
    <script src="~/Scripts/Auditoria/TipoEmpresa/CreateTipoEmpresa.js"></script>
    <script src="~/Scripts/sweetalert2@9.js"></script>
</head>
<body>
    <div class="col-md-12" style="background: rgb(248, 248, 248); width:1200px;left: 100px;">
        <br />
        <ul class="col-md-offset-2 nav nav-tabs" style="margin-top:0%;border: none; margin-left: 2%;">
            <li><a href="/Auditoria/Auditoria" class="tabsnota"><i class="fa fa-file-text" aria-hidden="true"></i> Registrar Empresa </a></li>
            <li class="active"><a href="/Auditoria/Auditoria/IndexTipoEmpresa" class="tabsnota"><i class="fa fa-file-text" aria-hidden="true"></i> Tipos de Empresa </a></li>
        </ul>
        <div class="tab-content contenedortabs">
            <div id="menu1" class="tab-pane fade in active">
                <br />
                <div class="row">
                    <div class="form-group">
                        <div class="col-md-8"><h3 style="border-bottom: 5px solid #2B7785 !important; width: 1150px; color: #2B7785 !important; padding: 9px 20px 15px 0px !important;margin-bottom: 31px !important;">CREAR TIPO DE EMPRESA</h3></div>
                        <div class="col-md-2" style=" margin-top:35px;">
                            <a href="/Auditoria/Auditoria/IndexTipoEmpresa" title="Atras">
                                <i class="fa fa-arrow-circle-o-left fa-2x " aria-hidden="true">Volver</i>
                            </a>
                        </div>
                    </div>
                </div>

                <div class="row">
                    @using (Html.BeginForm("CreateTipoEmpresa", "Auditoria", new { area = "Auditoria" }, FormMethod.Post, new { id = "theForm" }))
                    {
                        @Html.AntiForgeryToken()
                        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

                        
                            <div class="col-lg-4 col-lg-offset-1">
                                <div class="form-group">
                                    <label class="control-label col-md-10">TIPO DE EMPRESA</label>
                                    <div class="col-md-12">
                                        @Html.EditorFor(model => model.tipo, new { htmlAttributes = new { @class = "form-control" } })
                                    </div>
                                </div>
                            </div>


                    }

                    <div class="col-xs-12 col-lg-1">
                        <br />
                        <button class="btn btn-success" id="guardar">Guardar</button>
                    </div>
                    <br />
                  
                </div>
                <br />
            </div>
        </div>
        <br />
    </div>
</body>
</html>










